<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

    <item
        android:height="50dp"
        android:width="50dp">
        <shape>
            <solid
                android:color="@color/white" />
        </shape>
    </item>
    <item
        android:height="15dp"
        android:width="15dp"
        android:gravity="center">
        <shape>
            <solid
                android:color="@color/greenHighlight"/>
            <corners
                android:radius="15dp"/>
        </shape>
    </item>
</layer-list>